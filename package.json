{
    "name": "nexpect",
    "scripts": {
        "pretest": "run-s \"zarro update-git-submodules\"",
        "test": "run-s \"zarro test-dotnet\"",
        "pack": "run-s \"zarro pack\"",
        "cover": "run-s \"zarro build-cover-report\"",
        "build": "run-s \"zarro build\"",
        "build-with-msbuild": "cross-env BUILD_EXCLUDE=src/PeanutButter/**/* zarro build",
        "zarro": "cross-env DOTNET_CORE=1 BUILD_EXCLUDE=src/PeanutButter/**/* PACK_INCLUDE_CSPROJ=* PACK_EXCLUDE_CSPROJ=*Tests*,CoreConsumer,src/PeanutButter/**/* TEST_EXCLUDE=src/PeanutButter/**/* zarro",
        "prerelease": "run-s test",
        "release": "cross-env BUILD_CONFIGURATION=BuildForRelease run-s \"zarro release\""
    },
    "devDependencies": {
        "chalk": "^3.0.0",
        "cross-env": "^7.0.2",
        "debug": "^4.1.1",
        "del": "^5.1.0",
        "npm-run-all": "^4.1.5",
        "simple-git": "^1.132.0",
        "zarro": "^1.83.0"
    }
}
