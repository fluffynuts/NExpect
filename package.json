{
    "name": "nexpect",
    "scripts": {
        "pretest": "run-s \"zarro update-git-submodules\"",
        "test": "run-s quick-test",
        "quick-test": "run-s \"zarro test-dotnet\"",
        "pack": "run-s \"zarro pack\"",
        "cover": "run-s \"zarro build-cover-report\"",
        "build": "run-s \"zarro build\"",
        "build-with-msbuild": "cross-env BUILD_EXCLUDE=src/PeanutButter/**/* zarro build",
        "zarro": "cross-env DOTNET_CORE=1 BUILD_EXCLUDE=src/PeanutButter/**/* PACK_INCLUDE_CSPROJ=* PACK_EXCLUDE_CSPROJ=*Tests*,CoreConsumer,src/PeanutButter/**/* TEST_EXCLUDE=src/PeanutButter/**/* zarro",
        "prerelease": "run-s verify-up-to-date test clear-old-packages",
        "verify-up-to-date": "zarro verify-up-to-date",
        "release": "cross-env BUILD_CONFIGURATION=BuildForRelease run-s \"zarro release\"",
        "prerelease-beta": "run-s verify-up-to-date test clear-old-packages",
        "release-beta": "cross-env PACK_INCREMENT_VERSION=1 VERSION_INCREMENT_STRATEGY=prerelease BUILD_CONFIGURATION=BuildForRelease run-s \"zarro release\"",
        "clear-old-packages": "rimraf packages",
        "test-push": "run-s \"zarro test-push\""
    },
    "devDependencies": {
        "chalk": "^3.0.0",
        "cross-env": "^7.0.2",
        "debug": "^4.1.1",
        "del": "^5.1.0",
        "npm-run-all": "^4.1.5",
        "simple-git": "^3.16.0",
        "yafs": "^1.23.0",
        "zarro": "^1.131.0"
    }
}
