{
    "name": "nexpect",
    "scripts": {
        "pretest": "zarro update-git-submodules",
        "experiment": "zarro experiment-task",
        "test": "zarro test-dotnet",
        "pack": "zarro @",
        "cover": "zarro build-cover-report",
        "build": "zarro @",
        "build-with-msbuild": "cross-env BUILD_EXCLUDE=src/PeanutButter/**/* zarro build",
        "prerelease": "run-s verify-up-to-date test clear-old-packages",
        "verify-up-to-date": "zarro verify-up-to-date",
        "release": "cross-env BUILD_CONFIGURATION=BuildForRelease zarro release",
        "prerelease-beta": "run-s verify-up-to-date test clear-old-packages",
        "release-beta": "cross-env PACK_INCREMENT_VERSION=1 VERSION_INCREMENT_STRATEGY=prerelease BUILD_CONFIGURATION=BuildForRelease zarro release",
        "clear-old-packages": "rimraf packages",
        "test-push": "zarro @"
    },
    "devDependencies": {
        "chalk": "^3.0.0",
        "cross-env": "^7.0.2",
        "debug": "^4.1.1",
        "del": "^5.1.0",
        "npm-run-all": "^4.1.5",
        "simple-git": "^3.16.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "yafs": "^1.23.0",
        "zarro": "^1.144.71"
    }
}
